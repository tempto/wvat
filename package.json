{
  "name": "wvat",
  "description": "Web Vulnerability Assessment Tool",
  "version": "1.1.0",
  "author": "LDSO-FEUP-T2G1-2019",
  "repository": "https://gitlab.com/feup-tbs/ldso1920/t2g1",
  "bin": {
    "wvat": "./bin/run"
  },
  "bugs": "https://github.com/feup-tbs/wvat/issues",
  "dependencies": {
    "@apexearth/egrep": "^1.0.7",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@sentry/node": "^5.7.1",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cli-ux": "^5.3.2",
    "js-crawler": "^0.3.21",
    "node-persist": "^3.0.5",
    "csv-parse": "^4.7.0",
    "wappalyzer": "^5.8.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/html-reporter": "^2.1.0",
    "@stryker-mutator/javascript-mutator": "^2.1.0",
    "@stryker-mutator/jest-runner": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-jest": "^22.19.0",
    "fast-check": "^1.18.0",
    "jest": "^24.9.0",
    "jshint": "^2.10.2",
    "log4js": "^5.3.0",
    "retire": "^2.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://gitlab.com/feup-tbs/ldso1920/t2g1",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "hooks": {
      "init": [
        "./src/hooks/setupSentry"
      ]
    },
    "bin": "wvat",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "repositoryPrefix": "https://gitlab.com/feup-tbs/ldso1920/t2g1/blob/master/<%- commandPath %>"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --max-warnings 0",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-mutation": "stryker run",
    "code-quality": "jshint src --reporter=checkstyle > reports/code_quality.xml",
    "security-scan": "retire --js --outputpath reports/retire_security_report.json --outputformat json"
  }
}
